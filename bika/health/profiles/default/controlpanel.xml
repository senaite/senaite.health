<?xml version="1.0"?>
<!-- This creates an action in Site Setup control panel in Plone. Actions are
  bundled in categories. In Plone 4, only "Member", "Plone" and "Products" are
  supported. Senaite core adds the category "bika", that renders the
  "LIMS Configuration" section" in control panel.
-->
<object name="portal_controlpanel"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">

 <!-- Link "Aetiologic Agents" in "LIMS configuration" control panel -->
 <configlet
    title="Aetiologic Agents"
    action_id="bika_aetiologicagents"
    appId="senaite.health"
    category="bika"
    condition_expr=""
    icon_expr="string:++resource++bika.health.images/aetiologicagent.png"
    url_expr="string:$portal_url/bika_setup/bika_aetiologicagents"
    visible="True"
    i18n:domain="senaite.health"
    i18n:attributes="title">
    <permission>senaite.core: Manage Bika</permission>
 </configlet>

