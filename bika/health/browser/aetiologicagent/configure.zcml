<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="bika.health">

    <browser:page
      for="*"
      name="getaetiologicagents"
      class=".getaetiologicagents.ajaxGetAetiologicAgents"
      permission="zope.Public"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
      for="*"
      name="getaetiologicagentsubtypes"
      class=".getaetiologicagentsubtypes.ajaxGetAetiologicAgentSubtypes"
      permission="zope.Public"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

</configure>
